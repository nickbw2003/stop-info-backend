{
  "name": "stop-info-backend",
  "version": "0.0.1",
  "description": "backend for StopInfo applications",
  "main": "dist/server.js",
  "engines": {
    "node": ">=12.14.0"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "build": "tsc",
    "test": "jest",
    "start": "ts-node src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rm -rf dist && npm run build && cp -r src/public dist/public",
    "start:prod": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickbw2003/stop-info-backend.git"
  },
  "keywords": [
    "StopInfo",
    "backend"
  ],
  "author": "nickbw2003",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/supertest": "^2.0.8",
    "@nestjs/testing": "^6.10.13",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.13",
    "@nestjs/core": "^6.10.13",
    "@nestjs/platform-express": "9.2.1",
    "dotenv": "^8.2.0",
    "public-transport-js": "^0.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  }
}
